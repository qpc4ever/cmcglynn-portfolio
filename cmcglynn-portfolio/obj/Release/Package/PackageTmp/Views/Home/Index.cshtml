                <input type="number" style="color:black" placeholder="Enter number" id="testNumber" onkeydown = "if (event.keyCode == 13) document.getElementById('Run').click()" />
                <button id="Run" onclick="numFac()" style="color:black; border-radius:8px">Execute</button>
                <div id="total"></div>
                <pre class="brush: js">
            function numFac() {
            var i, num, fact;
            fact = 1;
            num = Number(document.getElementById("testNumber").value);

            for (i = 1; i <= num; i++) {
                fact = fact * i;
            }
            document.getElementById("total").innerHTML = fact;
        }
